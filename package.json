    "lodash": "^3.3.1"
  },
  "devDependencies": {
    "babel": "^4.6.1",
    "babel-tape-runner": "^1.2.0",
    "babelify": "^6.1.3",
    "browserify": "^9.0.3",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.2",
    "tape-catch": "^1.0.4",
    "watchify": "^3.3.1"
  }
